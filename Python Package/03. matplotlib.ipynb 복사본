   "source": [
    "x = np.arange(-5, 5, .1)\n",
    "y1 = x**2-2\n",
    "y2 = 20*np.cos(x)**2\n",
    "\n",
    "plt.figure(1)\n",
    "plt.plot(x, y1)\n",
    "\n",
    "plt.figure(2)\n",
    "plt.plot(x,y2)\n",
    "\n",
    "plt.figure(1)\n",
    "plt.plot(x,y2)\n",
    "\n",
    "plt.figure(2)\n",
    "plt.clf()\n",
    "plt.plot(x,y1)\n",
    "\n",
    "plt.show()"
   ]
  },
